<?xml version="1.0" encoding="utf-8"?>
<Application x:Class="LambdaLauncher.App" xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" StartupUri="Window/MainWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary x:Name="CurrentLanguage" Source="pack://application:,,,/Language/zh_cn.xaml" />
                <ResourceDictionary x:Name="CurrentTheme" Source="pack://application:,,,/Resource/Themes/Bamboo.xaml" />
                <ResourceDictionary Source="pack://application:,,,/Resource/Themes/GlobalColors.xaml" />
                <ResourceDictionary Source="pack://application:,,,/Resource/Styles/Buttons.xaml" />
                <ResourceDictionary Source="pack://application:,,,/Resource/Styles/Windows.xaml" />
                <ResourceDictionary Source="pack://application:,,,/Resource/Styles/Borders.xaml" />
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>