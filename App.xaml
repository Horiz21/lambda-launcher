<?xml version="1.0" encoding="utf-8"?>
<Application x:Class="LambdaLauncher.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             Startup
             ="Window/MainWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary x:Name="CurrentLanguage" Source="../Properties/Languages/zh_Hans.xaml" />
                <ResourceDictionary x:Name="CurrentTheme" Source="../Properties/Themes/bmbo.xaml" />
                <ResourceDictionary x:Name="CurrentMode" Source="../Properties/Themes/LightMode.xaml" />
                <ResourceDictionary Source="../Properties/Themes/GlobalColors.xaml" />
                <ResourceDictionary Source="../Properties/Styles/Components.xaml" />
                <ResourceDictionary Source="../Properties/Styles/Windows.xaml" />
                <ResourceDictionary Source="../Properties/Styles/Borders.xaml" />
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>