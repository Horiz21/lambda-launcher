<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <SolidColorBrush x:Key="Background" Color="{StaticResource DarkColor}" />
    <SolidColorBrush x:Key="Foreground" Color="{StaticResource LightColor}" />
    <SolidColorBrush x:Key="BackgroundStrong" Color="{StaticResource Black}" />
    <SolidColorBrush x:Key="ForegroundStrong" Color="{StaticResource White}" />
</ResourceDictionary>